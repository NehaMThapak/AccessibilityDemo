<!DOCTYPE html>
<html lang="en">
<% include ../includes/head.html.ejs %>
<body>

<div id="page">
	<% include ../includes/skipContent.html.ejs %>

	<% include ../includes/browseHappy.html.ejs %>

	<% include ../includes/header.html.ejs %>

	<!-- Main Content -->
	<main id="global-content" class="has-segment-nav <%= segment %>">

		<%
		var backLinkText = 'About RealEstate';
		var backLinkUrl = '/about';
		var pageTitle = 'Support';
		%>
		<% include ../includes/segment-nav.html.ejs %>

		<div class="content">
			<div class="container-fluid">

				<% rowType = 'alt'; %>
				<div class="row<%= (rowType)? (' ' + rowType) : '' %>">
					<div class="col-xs-12">
						<div class="page-header">
							<h1>
								<%- pageTitle %>
							</h1>
						</div>
					</div>
				</div>

				<% rowType = 'alt'; %>
				<div class="row<%= (rowType)? (' ' + rowType) : '' %>">
					<div class="col-xs-12">
						<div class="content-tile">
							<div class="row">
								<div class="col-xs-12 col-md-6">
									<h3>For Members</h3>
									<ul class="list-unstyled">
										<li>
											<strong>I’m a consumer</strong>
											<div>Got issues? We can help. Complete the form and our support team will get back to you in a jiffy.</div>
											<div>310-123-0164</div>
										</li>
										<li>
											<strong>I’m an agent</strong>
											<div>Are you an agent and need some assistance? Complete the form and our support team will be in touch shortly.</div>
											<div>949-345-0164</div>
										</li>
										<li>
											<strong>I have feedback</strong>
											<div>Whether you are an agent or a consumer, if you have an idea on how we can improve, we want to hear it!</div>
											<div>949-233-0164</div>
										</li>
									</ul>
								</div>
							</div>
						</div>
					</div>
				</div>

				<% rowType = 'alt'; %>
				<div class="row<%= (rowType)? (' ' + rowType) : '' %>">
					<div class="col-xs-12">
						<div class="content-tile">
							<%
							var contentPreviewTitle;
							var contentPreviewHeaderColor;
							var contentPreviewLink;
							var contentPreviewIsExternal;
							[
								{
									title: 'Be Well 24/7<sup>SM</sup>',
									headerColor: 'green',
									link: 'http://www.wellmark.com/HealthAndWellness/BeWell247',
									isExternal: true
								}
							].forEach(function (item) {
								contentPreviewTitle = item.title;
								contentPreviewHeaderColor = item.headerColor;
								contentPreviewLink = item.link;
								contentPreviewIsExternal = !!item.isExternal;
							%>
							<% include ../includes/content-preview-tile.html.ejs %>
							<%
							});
							%>
						</div>
					</div>
				</div>
			</div>
		</div>
	</main>

	<% include ../includes/footer.html.ejs %>
</div>

<% include ../includes/foot.html.ejs %>

</body>
</html>
